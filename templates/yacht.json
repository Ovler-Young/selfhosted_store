      {
        "139": "139:139/tcp",
        "445": "445:445/tcp"
      }
    ],
    "volumes": [
      {
        "container": "/mount",
        "bind": "/yacht"
      }
    ],
    "env": [
      {
        "name": "SHARE",
        "label": "SHARE",
        "default": "yacht;/mount"
      }
    ]
  },
