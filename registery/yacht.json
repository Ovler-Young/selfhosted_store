[
  {
    "type": 1,
    "title": "Yarr",
    "name": "yarr",
    "description": "开源 RSS 阅读器，Go 实现，数据存储于 SQLite。",
    "categories": [
      "Read"
    ],
    "platform": "linux",
    "note": "通过 IP:7070 打开。",
    "image": "yangkghjh/yarr:latest",
    "restart_policy": "unless-stopped",
    "ports": [
      {
        "7070": "7070:7070/tcp"
      }
    ],
    "volumes": [
      {
        "container": "/data",
        "bind": "!data/yarr"
      }
    ]
  }
]
